<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.zachmarcelo.mypurse"
   >


    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:name=".App"
        tools:replace="android:label"
        android:allowBackup="true"
        android:icon="@drawable/logo3puso"
        android:label="@string/app_name2"
        android:roundIcon="@drawable/logo3puso"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MeasureActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".AddNoteActivity" />
        <activity android:name=".NotePreviewActivity" />
        <activity android:name=".AddAlarmActivity" />
        <activity android:name=".StopRingtone" />

        <receiver android:name=".MyBroadcastReceiver" />
        <service android:name=".RingtonePlayingService" />
    </application>

</manifest>